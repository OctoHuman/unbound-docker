#!/usr/bin/env python3
try:
    import sys
    import os

    if len(sys.argv) != 2:
        sys.stderr.write("Exactly one parameter must be passed to this program.")
        sys.exit(1)

    # Prints the environment variable requested.
    # If the variable doesn't exists, it defaults to empty string
    sys.stdout.write(
        os.environ.get(sys.argv[1], "")
    )
except:
    pass